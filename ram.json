{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "ram.json#",
    "type": "object",
    "properties": {
        "name": {
            "type":"string"
        },
        "format": {
            "type": "string",
            "enum": ["ddr3", "ddr4", "ddr4 sodimm"]
        },
        "freq": {
            "type":"integer"
        },
        "size": {
            "type": "integer"
        },
        "price": {
            "type":"number"
        }
    },
    "required": ["format", "freq", "size", "price"]
}